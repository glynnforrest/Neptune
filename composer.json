{
    "name": "glynnforrest/neptune",
    "description": "A PHP 5.4+ framework built on the Symfony HttpKernel.",
    "keywords": ["framework", "neptune", "http-kernel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Glynn Forrest",
            "email": "me@glynnforrest.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "danielstjules/stringy": "~1.5",
        "glynnforrest/crutches": "0.3.*",
        "glynnforrest/reform": "0.4.*",
        "glynnforrest/blockade": "0.2.*",
        "glynnforrest/active-doctrine": "0.2.*",
        "symfony/http-kernel": "~2.4",
        "symfony/console": "~2.4",
        "symfony/yaml": "~2.4",
        "pimple/pimple": "~3.0",
        "monolog/monolog": "1.*",
        "doctrine/cache": "1.3.*",
        "doctrine/dbal": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "glynnforrest/temping": "0.5.*",
        "ircmaxell/password-compat": "1.0.*",
        "swiftmailer/swiftmailer": "~5.3"
    },
    "suggest": {
        "sensiolabs/security-checker": "Add a command to check for security vulnerabilities"
    },
    "autoload": {
        "psr-4": {
            "Neptune\\": "src/Neptune"
        }
    },
    "bin": ["bin/neptune-install"],
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    }
}
